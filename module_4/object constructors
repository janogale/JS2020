let student = {
  name: "ali",
  course: "js"
}

function Student(name, id){
  this.name = name;
  this.id = id; 
  this.course = "javascript";  
  this.year = 2020;
  this.isPaid = false;
  this.payFee = function(){
    this.isPaid = true
  }
}

let s1 = new Student("Mahamed", 101)
let s2 = new Student("Asma", 102)
let str = "hello"

